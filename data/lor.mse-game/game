mse version: 2.0.0
short name: LoR
full name: Legends of Runeterra
installer group: Legends of Runeterra/game files
icon: card-sample.png
position hint: 03

version: 2022-09-05

# Author : kino
# Most stuff is copy/pasted from magic.mse-game (and vanguard.mse-game! thanks Wolfwood)
############################################################## Functions & filters
# General functions
init script:
	
	############################################################## The text box
	
	potential_space := { if set.add_space_after_color_code then " " else "​" }
	
	text_filter := 
		replace@(
			match: "(\\[.+?\\])",
		 	replace: { "<color:rgb(250, 214, 90)>" + substring(input, begin:1, end:length(input) - 1) + "</color>" + potential_space() }) +
		replace@(
		 	match: "(\\\{.+?\\\})",
		 	replace: { "<color:rgb(73, 160, 248)>" + substring(input, begin:1, end:length(input) - 1) + "</color>" + potential_space() }) +
		replace@(
			match: "(\\(.\\))",
		 	replace: { "<sym>" + substring(input, begin:1, end:length(input) - 1) + "</sym>" }) +
		replace@(
		 	match: "(\\*.+?\\*)",
		 	replace: { remove_tag(tag: "<color:rgb(250, 214, 90)>", remove_tag(tag: "<color:rgb(73, 160, 248)>", remove_tag(tag: "</color>", substring(input, begin:1, end:length(input) - 1)))) })

	template_type := {
		if substring(stylesheet.short_name, end:8) == "Champion" then "Champion"
		else if substring(stylesheet.short_name, end:8) == "Follower" then "Follower"
		else if (substring(stylesheet.short_name, end:5) == "Spell") and (stylesheet.short_name != "Spell Splash") then "Spell"
		else if substring(stylesheet.short_name, end:8) == "Landmark" then "Landmark"
		else stylesheet.short_name }
	type_line := {
		if template_type() == "Pre-rendered" then value
		else if template_type() == "Champion" then (
			if styling.set_level == "Level 2" then
				"Unit — Champion (Lvl.2)"
			else "Unit — Champion (Lvl.1)" )
		else if template_type() == "Follower" then
			"Unit — Follower"
		else if template_type() == "Spell" then (
			v := card.keysymbolreal
			if styling.no_of_keywords != 1 then (
				v := card.keysymbol1
				if substring(v, end:5) != "Spell" then (
					v := card.keysymbol2
					if substring(v, end:5) != "Spell" then (
						v := card.keysymbol3
						if substring(v, end:5) != "Spell" then (
							v := card.keysymbol4
						)
					)
				)
			)
			if styling.set_spell_speed_frame == "Burst" or (styling.set_spell_speed_frame == "Default" and v == "Spell Burst") then "Spell - Burst"
			else if styling.set_spell_speed_frame == "Focus" or (styling.set_spell_speed_frame == "Default" and v == "Spell Focus") then "Spell - Focus"
			else if styling.set_spell_speed_frame == "Fast" or (styling.set_spell_speed_frame == "Default" and v == "Spell Fast") then "Spell - Fast"
			else if styling.set_spell_speed_frame == "Skill" or (styling.set_spell_speed_frame == "Default" and v == "Spell Skill") then "Skill"
			else if styling.set_spell_speed_frame == "Trap" or (styling.set_spell_speed_frame == "Default" and v == "Spell Trap") then "Trap"
			else if styling.set_spell_speed_frame == "Boon" or (styling.set_spell_speed_frame == "Default" and v == "Spell Boon") then "Trap"
			else if styling.set_spell_speed_frame == "Origin" or (styling.set_spell_speed_frame == "Default" and v == "Spell Origin") then "Origin"
			else if styling.set_spell_speed_frame == "Equipment" or (styling.set_spell_speed_frame == "Default" and v == "Spell Equipment") then "Equipment"
			else "Spell - Slow"
			)
		else if template_type() == "Unit Splash" then
			"Splash — Unit"
		else if template_type() == "Spell Splash" then
			"Splash — Spell"
		else template_type() }
		
	keyword_no := {
		if (template_type() == "Spell" or template_type() == "Landmark") then styling.no_of_keywords
		else styling.number_of_keywords
	}

	keyword_filter := {
		ret := value
		if (set.automatic_caps) then ret := to_upper(ret)
		if (template_type() == "Tooltip") or (template_type() == "Unit Splash") or (template_type() == "Spell Splash") then
			ret
		else if keyword_no() == 2 then 
			if card.keysymbol1 == "Standard Can't Attack" and card.keysymbol2 == "Standard Can't Block" and set.automatic_keyword_text then 
				"IMMOBILE"
			else ret
		else if substring(card.keysymbolreal, end:5) == "Spell" and set.automatic_keyword_text then
			to_upper(substring(card.keysymbolreal, begin:6))
		else if substring(card.keysymbolreal, end:8) == "Standard" and set.automatic_keyword_text then
			to_upper(substring(card.keysymbolreal, begin:9))
		else if card.keysymbolreal == "Landmark" and set.automatic_keyword_text then
			"LANDMARK"
		else ret
	}
		
	get_darkness := {
		if styling.set_shadow == "Light" then 0.25
		else if styling.set_shadow == "Moderate" then 0.5
		else if styling.set_shadow == "Heavy" then 0.75
		else if styling.set_shadow == "Extreme" then 1
		else 0
	}

	############################################################## Other boxes
	
	# Process the name for sorting rules
	sort_name :=
		# Remove "The", "A", and "And" at the beginning
		replace@(match: "^(The|An?) ", replace: "") +
		# Remove commas and apostrophes
		replace@(match: "(,|'|’)", replace: "") +
		# Remove bold and italic tags
		replace@(match: "(<b>|<i>|</b>|</i>)", replace: "") +
		# Make lowercase
		to_lower
	
	word_count := break_text@(match:"[^[:space:]]+") + length
	line_count := split_text@(match:"\n+",include_empty:false) + length
	
############################################################## Set fields
set field:
	type: info
	name: Set Information
set field:
	type: text
	name: title
	description: The name of the set.
set field:
	type: text
	name: description
	multi line: true
	description: The summary of the set.
set field:
	type: text
	name: artist
	description: Enter an artist's name to have it appear in the bottom left of all cards by default.
set field:
	type: text
	name: copyright
	description: Enter a designer's name to have it appear in the bottom right of all cards.
set field:
	type: info
	name: Defaults and Automation
set field:
	type: boolean
	name: use new region placement
	initial: yes
	description: Places region icons in their own box in the top right, rather than on the bottom of the card.
set field:
	type: boolean
	name: automatic caps
	initial: yes
	description: Automatically converts card names, subtypes and keyword boxes to full caps. Doesn't apply to tooltips names.
set field:
	type: boolean
	name: use shadow
	initial: yes
	description: Adds a dark shadow between art and text on unit templates to improve clairty. The opacity can be edited card by card in the Style tab.
set field:
	type: boolean
	name: use colour prompts
	initial: yes
	description: Prompts the user to add colour to spells if the default colour hasn't yet been changed.
set field:
	type: boolean
	name: add space after color code
	initial: no
	description: Adds a space after adding a color code to ability text.
set field:
	type: boolean
	name: automatic keyword text
	initial: yes
	description: Automatically fills the keyword text box according to the keyword symbol used.
set field:
	type: boolean
	name: automatic card numbers
	initial: no
	description: This is actually useless but the code breaks if I remove it so yeah
set field:
	type: info
	name: Custom Links
set field:
