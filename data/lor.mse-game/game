card field:
	type: choice
	name: region
	card list visible: true
	card list column: 3
	card list width: 100
	description: The region of the card
	script:
		if (template_type() == "Tooltip") then ""
		else if (value == "") then "Runeterra"
		else value
	icon: stats/card_type.png
	choice: 
		name: None
		line below: true
	choice: Demacia
	choice: Freljord
	choice: Ionia
	choice: Noxus
	choice: Piltover & Zaun
	choice: Shadow Isles
	choice: Bilgewater
	choice: Targon
	choice: Shurima
	choice: Bandle City
	choice: 
		name: Runeterra
		line below: true
	choice: Ixtal
	choice: The Void
	choice: Kills
	choice: Deaths
	choice: 
		name: Assists
		line below: true
	choice: 
		name: Custom
		choice: Region 1
		choice: Region 2
		choice: Region 3
		choice: Region 4
		choice: Region 5
	initial: Runeterra
