	type: color
	name: Level Condition Color
	initial: rgb(223,154,122)
	choice:
		name: Default
		color: rgb(223,154,122)
	description: Sets the color of level up text.
set field:
	type: color
	name: Stats Color
	initial: rgb(255, 255, 255)
	choice:
		name: Default
		color: rgb(255, 255, 255)
	description: Sets the color of cost, power and health digits.
set field:
	type: color
	name: Grouping Color
	initial: rgb(173,168,157)
	choice:
		name: Default
		color: rgb(173,168,157)
	description: Sets the color of subtypes shown at the top of cards.
set field:
	type: color
	name: Token Color
	initial: rgb(120,120,110)
	choice:
		name: Default
		color: rgb(120,120,110)
	description: Sets the color of token tags shown at the bottom of token spells, usually Treasures.
set field:
	type: color
	name: Illustrator and Designer Color
	initial: rgb(0, 0, 0)
	choice:
		name: Default
		color: rgb(0, 0, 0)
	description: Sets the color of artist and designer names.
