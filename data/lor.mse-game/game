set field:
	type: info
	name: Card Fonts
set field:
	type: text
	name: Title Font
	description: Enter the name of a font to use for the title of cards, or leave blank to default to BeaufortforLOL-Bold (if installed)
set field:
	type: text
	name: Keyword Font
	description: Enter the name of a font to use for keywords and for spell token types, or leave blank to default to BeaufortforLOL-Bold (if installed)
set field:
	type: text
	name: Text Font
	description: Enter the name of a font to use ability text of cards, or leave blank to default to Univers 55 (if installed)
set field:
	type: text
	name: Stats Font
	description: Enter the name of a font to use cost, power and health, or leave blank to default to BeaufortforLOL-Bold (if installed)
set field:
	type: text
	name: Grouping Font
	description: Enter the name of a font to use the subtype at the top of cards and for the title of tooltips, or leave blank to default to Univers LT Std 59 UltraCn (if installed)
set field:
	type: text
	name: Illustrator and Designer Font
	description: Enter the name of a font to use for artist and designer credit, or leave blank to default to BeaufortforLOL-Bold (if installed)
set field:
	type: info
	name: Card Text Colors
set field:
	type: color
	name: Title Color
	initial: rgb(255, 255, 255)
	choice:
		name: Default
		color: rgb(255, 255, 255)
	description: Sets the color of card titles.
set field:
	type: color
	name: Keyword Color
	initial: rgb(240,204,112)
	choice:
		name: Default
		color: rgb(240,204,112)
	description: Sets the color of text in keyword boxes.
