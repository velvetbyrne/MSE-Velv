				else 148 + keyword_chop()
		top: { if conjoin() then keyword_height()*2 - 2 else keyword_height()*2 }
		width :
			script:
				if (styling.no_of_keywords > 1 and conjoin()) then 
					if (styling.no_of_keywords == "2") then
						444 - keyword_chop()*2
					else 120
