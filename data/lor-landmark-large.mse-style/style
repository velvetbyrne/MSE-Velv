card width: 680
card height: 1024
card dpi: 150
# Author: kino
# Based on: magic-new
############################################################## Extra scripts
init script:
	keyword_chop := {
		hack := 0
		if card.keysymbolreal == "Blank" and not (conjoin() and styling.no_of_keywords == "2") then hack := 26
		if styling.set_keyword_chop != "" then ( to_number(styling.set_keyword_chop) + hack )
		## else ( for each k:v in keyword_chops do  if k == card.keywordtext then v )
		else min( to_int((338 - card_style.keywordtext.content_width)/2) + hack, 150)
	}
	keyword_height := {
		x := 372
		if styling.no_of_keywords != 0 then (
			if styling.use_ability_text then x := x - 27
			x := x + the_great_offset() - ability_offset() - to_number(styling.set_keywords_offset) )
		x }
	name_text_lines := { max(1, (hight)/2 div 20) }
	text_lines := { max(1, (hight)/2 div 18) }
	font_size := { 
		if fld == "name" then
			if (styling.set_title_font_size != "" and styling.set_title_font_size != "-") then to_number(styling.set_title_font_size) else 22
		else if fld == "ability" then 
			if (styling.set_text_font_size != "" and styling.set_text_font_size != "-") then to_number(styling.set_text_font_size) else 12.5
		else 0
	}
	ability_offset := {
		tempoff := 6
		if styling.use_ability_text then (
			x := to_number(styling.set_ability_text_offset)
			x + text_lines(hight: card_style.ability.content_height)*20 - 20 + tempoff
		) else tempoff
	}
	the_great_offset := {
		if styling.use_ability_text then
			if text_lines(hight: card_style.ability.content_height) == 1 then 0 else 2	
		else 0		
	}
	conjoin := {
		if styling.conjoin_first_two_keyword_boxes == "Always" then true
		else if styling.conjoin_first_two_keyword_boxes == "Never" then false
		else ((styling.no_of_keywords > 1) and (card.keysymbol1 == "Standard Can't Attack") and (card.keysymbol2 == "Standard Can't Block"))		
	}

############################################################## Extra style options
styling field:
	type: boolean
	name: use ability text
	description: Toggles the visibility of the standard ability text.
	initial: true
styling field:
	type: choice
	name: no of keywords
	description: Set the number of keywords to use. Keyword text can only be if used if this is set to 1.
	choice: 1
	choice: 2
	choice: 3
	choice: 4
	initial: 1
styling field:
	type: choice
	name: number of regions
	description: Set the number of regions of the card. Only the first will be displayed in the cardlist.
	choice: 0
	choice: 1
	choice: 2
	choice: 3
	initial: 1
styling field:
	type: choice
	name: set shadow
	description: Sets amount of shadow between the art and the text.
	choice: None
	choice: Light
	choice: Moderate
	choice: Heavy
	choice: Extreme
	initial: Moderate
styling field:
	type: text
	name: set title font size
	description: Sets the font size of the card's name. Leave blank to default to 22.
styling field:
	type: text
	name: set text font size
	description: Sets the font size of the card's ability text. Leave blank to default to 12.5.
styling field:
	type: text
	name: set keyword chop
	description: Overrides the regular sizing of the keyword box. Automatic resizing is disabled if this field contains a value.
styling field:
	type: text
	name: set title offset
	description: Adjusts the position of the name.
styling field:
	type: text
	name: set keywords offset
	description: Adjusts the position of the name and keywords.
styling field:
	type: text
	name: set ability text offset
	description: Adjusts the position of the ability text and everything above.
styling field:
	type: choice
	name: conjoin first two keyword boxes
	description: Determines when the first and second keyword boxes are merged into one when two or more are displayed.
	initial: Only for "Can't Attack / Block"
	choice: Never
	choice: Only for "Can't Attack / Block"
	choice: Always
styling field:
	type: boolean
	name: use image background
	description: Determines whether to use a block colour background an image input background.
	initial: no
